import turtle


turtle.bgcolor("black")


pen = turtle.Turtle()
pen.color("red")
pen.pensize(3)
pen.speed(5)


pen.begin_fill()
pen.fillcolor("red")

pen.left(140)
pen.forward(180)
pen.circle(-90, 200)
pen.left(120)
pen.circle(-90, 200)
pen.forward(180)

pen.end_fill()


pen.hideturtle()


turtle.done()
